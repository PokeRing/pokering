{
  "name": "PokeRing",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=aws ./node_modules/forever/bin/forever start start.js",
    "stop": "./node_modules/forever/bin/forever stopall",
    "debug": "NODE_ENV=development DEBUG=PokeRing ./node_modules/gulp/bin/gulp.js",
    "migrate": "./node_modules/db-migrate/bin/db-migrate up --config config/database.json -e development",
    "migrate-test": "./node_modules/db-migrate/bin/db-migrate up --config config/database.json -e testing",
    "migrate-production": "./node_modules/db-migrate/bin/db-migrate up --config config/database.json -e production",
    "pull": "git fetch --all && git pull --rebase origin master && npm prune && npm install && npm run migrate-development",
    "pull-test": "git fetch --all && git pull --rebase origin master && npm prune && npm install && npm run migrate-test",
    "pull-production": "git fetch --all && git pull --rebase origin master && npm prune && npm install && npm run migrate-production",
    "test": "./node_modules/mocha/bin/mocha"
  },
  "dependencies": {
    "body-parser": "~1.0.0",
    "cookie-parser": "~1.0.1",
    "db-migrate": "^0.7.0",
    "debug": "~0.7.4",
    "express": "~4.2.0",
    "forever": "^0.11.1",
    "mocha": "^1.21.4",
    "morgan": "~1.0.0",
    "mysql": "^2.4.1",
    "nodeunit": "^0.9.0",
    "serve-favicon": "^2.0.1",
    "supertest": "^0.13.0"
  },
  "devDependencies": {
    "gulp": "^3.8.6",
    "gulp-jshint": "^1.7.1",
    "gulp-shell": "^0.2.8",
    "supervisor": "^0.6.0"
  }
}
